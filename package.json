{
    "name": "legalhub",
    "version": "1.0.0",
    "description": "A digital platform that allows users to rent items such as appliances,",
    "keywords": [
        "nodejs",
        "mongoose",
        "express",
        "backend"
    ],
    "license": "ISC",
    "author": "",
    "type": "commonjs",
    "main": "src/index.js",
    "scripts": {
        "test": "NODE_ENV=test SUPPRESS_CONSOLE=true jest --detectOpenHandles",
        "test:coverage": "NODE_ENV=test SUPPRESS_CONSOLE=true jest --coverage --passWithNoTests",
        "test:ci": "NODE_ENV=test SUPPRESS_CONSOLE=true jest --ci --coverage --passWithNoTests --reporters=default --reporters=jest-junit --outputFile=test-results/junit.xml",
        "test:suite": "node scripts/test-runner.js",
        "start": "node src/index.js",
        "dev": "nodemon src/index.js",
        "build": "echo 'No build step required for Express.js' && exit 0",
        "format": "prettier --write . && eslint --fix ."
    },
    "devDependencies": {
        "eslint": "^9.37.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "jest": "^30.2.0",
        "jest-junit": "^16.0.0",
        "mongodb-memory-server": "^10.2.2",
        "nodemon": "^3.1.10",
        "prettier": "^3.6.2",
        "supertest": "^7.1.4"
    },
    "dependencies": {
        "@google/genai": "^1.24.0",
        "axios": "^1.12.2",
        "bcrypt": "^6.0.0",
        "cloudinary": "^1.30.0",
        "compression": "^1.8.1",
        "connect-flash": "^0.1.1",
        "connect-mongo": "^5.1.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "ejs": "^3.1.10",
        "ejs-mate": "^4.0.0",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "express-flash": "^0.0.2",
        "express-mongo-sanitize": "^2.2.0",
        "express-rate-limit": "^8.1.0",
        "express-session": "^1.18.2",
        "express-useragent": "^1.0.15",
        "geoip-lite": "^1.4.10",
        "helmet": "^8.1.0",
        "hpp": "^0.2.3",
        "json2csv": "^6.0.0-alpha.2",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "method-override": "^3.0.0",
        "mongodb": "^6.20.0",
        "mongoose": "^8.19.0",
        "mongoose-aggregate-paginate-v2": "^1.1.4",
        "mongoose-paginate-v2": "^1.9.1",
        "morgan": "^1.10.1",
        "multer": "^2.0.2",
        "multer-storage-cloudinary": "^4.0.0",
        "nodemailer": "^7.0.7",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^8.0.0",
        "puppeteer": "^24.23.0",
        "qrcode": "^1.5.4",
        "sanitize-html": "^2.17.0",
        "shortid": "^2.2.17",
        "socket.io": "^4.8.1",
        "web-push": "^3.6.7",
        "xss-clean": "^0.1.4"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/test/jest.setup.js"
        ],
        "globalSetup": "<rootDir>/test/globalSetup.js",
        "globalTeardown": "<rootDir>/test/globalTeardown.js",
        "testEnvironment": "node",
        "testTimeout": 60000,
        "maxWorkers": 1,
        "forceExit": true,
        "collectCoverageFrom": [
            "src/**/*.js",
            "!src/index.js",
            "!src/config/**",
            "!src/views/**",
            "!src/public/**",
            "!src/readme.md"
        ],
        "coverageDirectory": "coverage",
        "coverageReporters": [
            "text",
            "lcov",
            "html"
        ],
        "reporters": ["default"]
    },
    "directories": {
        "test": "test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Vivekk4824/LegalHuB.git"
    },
    "bugs": {
        "url": "https://github.com/Vivekk4824/LegalHuB/issues"
    },
    "homepage": "https://github.com/Vivekk4824/LegalHuB#readme"
}
